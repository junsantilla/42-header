{
	"name": "42-header",
	"displayName": "42 Header - Universal",
	"description": "A standard 42 header that is required at the beginning of every source code you will write. \t",
	"publisher": "junsantilla",
	"version": "0.1.0",
	"icon": "42.png",
	"engines": {
		"vscode": "^1.86.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "42-header.insertComments",
				"title": "Show Comments"
			}
		],
		"keybindings": [
			{
				"command": "42-header.insertComments",
				"key": "ctrl+alt+h",
				"mac": "cmd+alt+h",
				"when": "editorTextFocus"
			}
		],
		"configuration": {
			"title": "42-header",
			"properties": {
				"42header.username": {
					"type": "string",
					"default": ""
				},
				"42header.email": {
					"type": "string",
					"default": ""
				},
				"42header.customDomain": {
					"type": "string",
					"default": ".fr",
					"description": "Custom domain for the 42-header extension"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.86.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"@vscode/test-cli": "^0.0.6",
		"@vscode/test-electron": "^2.3.9"
	}
}
